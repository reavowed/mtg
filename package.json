{
  "name": "mtg",
  "version": "0.1.0",
  "description": "Free MTG",
  "scripts": {
    "build": "concurrently \"npm:build-*\"",
    "build-js": "parcel build --dist-dir src/main/resources/static/js --public-url /js/ \"src/main/js/main.js\"",
    "build-css": "parcel build --dist-dir src/main/resources/static/css --public-url /css/ \"src/main/css/main.scss\"",
    "dev": "concurrently \"npm:dev-*\"",
    "dev-js": "parcel watch --dist-dir src/main/resources/static/js --public-url /js/ \"src/main/js/main.js\"",
    "dev-css": "parcel watch --dist-dir src/main/resources/static/css --public-url /css/ \"src/main/css/main.scss\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.13.13",
    "babel-core": "^6.26.3",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "concurrently": "^6.0.2",
    "parcel": "^2.0.0-rc.0",
    "sass": "^1.32.13"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "date-fns": "^2.21.3",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "preact": "^10.5.7",
    "react-beautiful-dnd": "^13.1.0",
    "react-bootstrap": "^1.6.0",
    "sockjs-client": "^1.5.1",
    "stompjs": "^2.3.3"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat"
  }
}
